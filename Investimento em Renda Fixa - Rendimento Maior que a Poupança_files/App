(function(){"use strict";var n=angular.module("app",["ngAnimate","ngSanitize","ngRoute","ui.router","ui.bootstrap","ui.jq","chart.js"]).directive("loading",["$http",function(n){return{restrict:"A",link:function(t,i){t.isLoading=function(){return n.pendingRequests.length>0};t.$watch(t.isLoading,function(n){n&&!i.is(":visible")?i.show():i.hide()})}}}])})()