var baseUrl=$("#BaseURL").val();(function(){var n=angular.module("app");n.controller("produtosController",["$scope","$http","$location",function(n,t){n.Produtos={};$(document).ready(function(){t({method:"POST",url:baseUrl+"/Home/RetornaProdutos",data:{},headers:{RequestVerificationToken:n.antiForgeryToken,"X-Requested-With":"XMLHttpRequest"}});n.CarregarProduto()});n.dynamicOrder=function(){return posicao[n.colunas]};n.CarregarProduto=function(){t({method:"POST",url:baseUrl+"/Home/RetornaProdutos",data:{},headers:{RequestVerificationToken:n.antiForgeryToken,"X-Requested-With":"XMLHttpRequest"}}).then(function(t){var i=t.data,r=t.status,u=t.statusText,f=t.headers,e=t.config;n.Produtos=i},function(n){var t=n.data,i=n.status,r=n.statusText,u=n.headers,f=n.config;Alerta("Produtos","Ocorreu uma falha ao carregar os produtos. Contate o administrador.")})};n.MostrarMais=function(){n.CarregarProduto()}}])})()